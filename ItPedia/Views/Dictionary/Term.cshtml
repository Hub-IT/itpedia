@model ItPedia.Models.Term

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Model.Name;
}

<div id="term">

    <a href="javascript:history.go(-1)" class="btn btn-default">Back</a> <a href="@Model.Source" class="btn btn-default">Source</a>

    <h2>@Model.Name</h2>

    @Html.Raw(Model.Content)
</div>